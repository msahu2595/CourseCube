{
  "name": "coursecube",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "studio": "open -a /Applications/'Android Studio.app'",
    "p4": "emulator -avd Pixel_4",
    "m31": "npx react-native run-android --deviceId=RZ8N61DYPPX",
    "ip12": "npx react-native run-ios --device 'Manishâ€™s iPhone 12'",
    "ip14": "npx react-native run-ios --simulator='iPhone 14'",
    "ipad": "npx react-native run-ios --simulator='iPad (10th generation)'",
    "list:android": "adb devices",
    "list:ios": "xcrun xctrace list devices",
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start --reset-cache",
    "preandroid:build": "npx jetify",
    "android:build": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug",
    "android:release": "npx react-native run-android --variant=release",
    "ios:release": "npx react-native run-ios --configuration=release",
    "copy:apk": "cp android/app/build/outputs/apk/release/app-release.apk",
    "copy:ipa": "cp ios/build/coursecube.ipa",
    "install:apk": "adb install android/app/build/outputs/apk/release/app-release.apk",
    "install:ipa": "ideviceinstaller -i ios/build/coursecube.ipa",
    "release:apk": "firebase appdistribution:distribute android/app/build/outputs/apk/release/app-release.apk --app 1:712761607011:android:571029fa68837ace4a4956",
    "release:ipa": "firebase appdistribution:distribute ios/build/coursecube.ipa --app 1:712761607011:ios:16c3ce7b86eb536f4a4956",
    "release": "yarn run release:apk && yarn run release:ipa",
    "clean:android": "rm -rf $TMPDIR/metro-* && watchman watch-del-all && rm -rf node_modules && yarn cache clean && yarn install && rm -rf android/build && rm -rf android/app/build  && (cd android && ./gradlew clean)",
    "clean:ios": "rm -rf $TMPDIR/metro-* && watchman watch-del-all && rm -rf node_modules && yarn cache clean && yarn install && rm -rf ios/build && rm -rf ios/Pods && pod cache clean --all && rm -rf ~/.cocoapods && (cd ios && pod update)",
    "postinstall": "patch-package",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-firebase/app": "^16.4.3",
    "@react-native-firebase/auth": "^16.4.3",
    "@react-native-firebase/messaging": "^16.4.3",
    "@react-native-google-signin/google-signin": "^8.2.1",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/material-top-tabs": "^6.3.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "dayjs": "^1.11.6",
    "graphql": "^16.6.0",
    "patch-package": "^6.5.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-animated-tree-view": "^0.1.4",
    "react-native-blob-util": "^0.17.0",
    "react-native-flash-message": "^0.3.1",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-pager-view": "^6.1.0",
    "react-native-pdf": "^6.6.2",
    "react-native-popup-menu": "^0.16.1",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-swiper-flatlist": "^3.0.17",
    "react-native-tab-view": "^3.3.0",
    "react-native-vector-icons": "^9.2.0",
    "twrnc": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
